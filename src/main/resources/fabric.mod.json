{
  "schemaVersion": 1,
  "id": "ctl-tiertagger",
  "version": "${version}",
  "name": "CTL TierTagger",
  "description": "Displays player PvP tiers from Central TierList directly in their nametags",
  "authors": [
    "CTL Team"
  ],
  "contact": {
    "homepage": "https://ctltierlist.vercel.app",
    "sources": "https://github.com/ctltierlist/tiertagger"
  },
  "license": "MIT",
  "icon": "assets/ctl-tiertagger/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.ctltierlist.tiertagger.CTLTierTaggerClient"
    ],
    "modmenu": [
      "com.ctltierlist.tiertagger.client.ModMenuIntegration"
    ]
  },
  "mixins": [
    "ctl-tiertagger.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.0",
    "minecraft": ">=1.21.0 <=1.21.11",
    "java": ">=21",
    "fabric-api": "*",
    "modmenu": ">=13.0.0"
  }
}
